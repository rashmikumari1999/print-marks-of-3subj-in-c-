#include <iostream>
using namespace std;
int main()
{
    int subj1,subj2,subj3,P;
    cout<<"enter the marks of three subject:";
    cin>>subj1>>subj2>>subj3;
    P=subj1+subj2+subj3/3;
    if(P>=40)
    cout<<"pass";
    else
    cout<<"Fail";
}
